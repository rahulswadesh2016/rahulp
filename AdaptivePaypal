                //            primary = false,
                //        }).Distinct().ToList();

            }
            //var rr = decimal.Round((decimal)rlst.ToList().Sum(a => a.Pfamt),1);
            //List<Receiver> rlst = new List<Receiver>();
            //Receiver r = new Receiver();
